<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows10.0.19041.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="ReactiveUI.SourceGenerators" Version="2.2.4" />
    <PackageReference Include="Velopack" Version="0.0.1251" />
    <PackageReference Include="AWSSDK.S3" Version="4.0.2" />
    <PackageReference Include="CrissCross.WPF.UI" Version="2.5.1" />
    <PackageReference Include="DotNetProjects.Extended.Wpf.Toolkit" Version="5.0.124" />
    <PackageReference Include="gong-wpf-dragdrop" Version="4.0.0" />
    <PackageReference Include="FluentValidation" Version="12.0.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\MultiSelectTreeView\MultiSelectTreeView.csproj" />
    <Resource Include="Images\*\*.*" />
    <Resource Include="Images\*.*" />
  </ItemGroup>

</Project>
